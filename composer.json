{
  "name": "mrdarkskil/amazon-service",
  "description": "Amazon service, find product' data and more...",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Leo Hubert",
      "email": "leo.hubert@epitech.eu"
    }
  ],
  "scripts": {
    "test": "./vendor/bin/phpunit tests"
  },
  "minimum-stability": "stable",
  "require": {
    "paquettg/php-html-parser": "^2.2",
    "guzzlehttp/guzzle": "^6.5"
  },
  "autoload": {
    "psr-4": {
