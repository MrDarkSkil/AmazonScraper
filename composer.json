{
  "name": "mrdarkskil/amazon-service",
  "description": "Amazon service, find product' data and more...",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Leo Hubert",
      "email": "leo.hubert@epitech.eu"
    }
  ],
  "scripts": {
    "test": "./vendor/bin/phpunit tests"
  },
  "minimum-stability": "stable",
  "require": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "AmazonService\\": "src/",
      "AmazonService\\Enums\\": "src/enums/",
      "AmazonService\\Exceptions\\": "src/exceptions/",
      "AmazonService\\Models\\": "src/models/",
      "AmazonService\\Services\\": "src/services/",
      "AmazonService\\Utils\\": "src/utils/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AmazonService\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  }
}
